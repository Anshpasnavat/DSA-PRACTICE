#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    int mySqrt(int x) {
        if (x == 0 || x == 1)
            return x;

        long long low = 1, high = x, ans = 0;

        while (low <= high) {
            long long mid = low + (high - low) / 2;

            if (mid <= x / mid) {   // mid*mid <= x (overflow-safe)
                ans = mid;
                low = mid + 1;
            } else {
                high = mid - 1;
            }
        }

        return ans;
    }
};

int main() {
    int x;
    cin >> x;

    Solution s;
    cout << s.mySqrt(x);

    return 0;
}
